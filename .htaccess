RewriteEngine On 
RewriteCond %{HTTPS} !=on 
RewriteRule .* https://www.bodegasf.com/%{REQUEST_URI} [R,L]

RewriteCond %{HTTP_HOST} !^www\. 
RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L] 

RewriteCond %{REQUEST_URI} ^/giftcard/.*
RewriteRule (.*) https://giftup.app/place-order/de23e7ad-2134-4c53-d2ee-08dad91f60f7?platform=hosted [R=301,L]
